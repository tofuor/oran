{
    "xapp_name": "python-savedata",
    "version": "1.0.0",
    "containers": [
        {
            "name": "python-savedata",
            "image": {
                "registry": "nexus3.o-ran-sc.org:10002",
                "name": "mitlab/ric-app-python-savedata",
                "tag": "1.0.0"
            }
        }
    ],
    "messaging": {
        "ports": [
            {
                "name": "rmr-data",
                "container": "python-savedata",
                "port": 4560,
                "rxMessages": ["HO_UE_LIST"],
                "txMessages": [],
                "policies": [],
                "description": "rmr receive data port for hw"
            },
            {
                "name": "rmr-route",
                "container": "python-savedata",
                "port": 4561,
                "description": "rmr route port for hw"
            }
        ]
    },
    "rmr": {
        "protPort": "tcp:4560",
        "maxSize": 2072,
        "numWorkers": 1,
        "rxMessages": ["HO_UE_LIST"],
        "txMessages": [],
        "policies": []
    }
}